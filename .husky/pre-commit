start_time=$(date +%s)

eslint --fix .

eslint_end_time=$(date +%s)
echo "\033[32m✅ Eslint检查通过 (耗时 $((eslint_end_time - start_time)) 秒)\033[0m"

prettier_start_time=$(date +%s)

prettier --write "**/*.{ts,tsx,js,jsx}" --loglevel silent

prettier_end_time=$(date +%s)
echo "\033[32m✅ Prettier格式化完成 (耗时 $((prettier_end_time - prettier_start_time)) 秒)\033[0m"