{
  "cli": {
    "version": ">= 10.2.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      }
    },
    "simulator": {
      "channel": "simulator",
      "ios": {
        "simulator": true
      }
    },
    "test": {
      "channel": "test",
      "env": {
        "GOOGLE_SERVICES_JSON": "${EAS_ENV.GOOGLE_SERVICES_JSON}"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store",
        "simulator": false // 设为 true 可以生成 iOS 模拟器包
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    }
  },
  "submit": {
    "production": {},
    "test": {
      "ios": {
        "ascAppId": "${EAS_ENV.ASC_APP_ID}"
      }
    },
    "development": {}
  }
}
