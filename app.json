{
  "expo": {
    "name": "华师匣子",
    "slug": "ccnucoursetable",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "wx6220588048f6e417",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "src/assets/images/",
      "src/assets/data/",
      "src/assets/fonts"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "release-20240916",
      "buildNumber": "8",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "我们需要访问您的照片库用于保存截图。",
        "NSPhotoLibraryAddUsageDescription": "我们需要将图片保存到您的照片库。",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "LSApplicationQueriesSchemes": ["weixin", "alipays", "tel", "sms"]
      }
    },
    "android": {
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.ACCESS_NOTIFICATION_POLICY",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.muxixyz.ccnubox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "resizeMode": "cover",
          "imageWidth": 300,
          "backgroundColor": "#7D82F4"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d6863b18-d443-4651-b836-5129afd64edc"
      }
    },
    "runtimeVersion": "1.2.0",
    "updates": {
      "url": "https://u.expo.dev/d6863b18-d443-4651-b836-5129afd64edc"
    },
    "owner": "muxi_fe"
  }
}
