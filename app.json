{
  "expo": {
    "name": "ccnu_coursetable",
    "slug": "ccnucoursetable",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/mx-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "assets/images/",
      "assets/data/",
      "assets/fonts"
    ],
    "eas": {
      "build": {
        "assetBundlePatterns": [
          "assets/images/",
          "assets/fonts",
          "assets/data/"
        ]
      }
    },
    "splash": {
      "image": "./assets/images/mx-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "release-20240916",
      "buildNumber": "3",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mx-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "googleServicesFile": "./secret/google-services.json",
      "package": "com.kbkbk.ccnu_coursetable"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mx-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d6863b18-d443-4651-b836-5129afd64edc"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d6863b18-d443-4651-b836-5129afd64edc"
    },
    "owner": "muxi_fe"
  }
}
